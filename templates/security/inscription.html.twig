
{% extends 'base.html.twig' %}
 {% block body %}
 <div class="container mt-3">
 <h2>Inscription  </h2>
 {{ form_start(form) }}


<div class="form-group">
{{ form_row(form.nomClient,{'attr':{'placeholder':'nomClient ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.cinClient,{'attr':{'placeholder':'cinClient ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.email,{'attr':{'placeholder':'email ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.password,{'label':'mot de passe','attr':{'placeholder':'mot de passe ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.adresse,{'label':'adresse','attr':{'placeholder':'adresse ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
<button type="submit" class="btn btn-primary">Inscrire</button>
</div>
{{ form_end(form) }}
</div>
{% endblock %}